displayedname() {
    # display the last bit, swap with capitalized hostname if localhost
    sed -e 's/^.*_//' -e "s/^localhost$/$(hostname | sed 's/[^ _-]*/\u&/g')/"
}

extra_exports=
while [ $# -gt 0 ]; do opt="$1"; case "$opt" in
    -f) {
        FAST=y
        shift
    }
    ;;

    -export) {
        extra_exports+=" $2 "
        shift
        shift
    }
    ;;

    -debug) {
        set -v
        export DEBUG_MODE=y
        shift
    }
    ;;

    *) break;
esac; done

editor() {
    if e running; then
        e c "$1"
    else
        v "$1"
    fi
}

init-pen-tm() {
    server="$(pen-tm-get-server)"

    TMUX= tmux -L $server attach -t localhost:
    tmux set-option -t localhost lock-after-time 36000
    echo init
    exit 0
}

        # pc|pipec|pipe-to-command|sf|select-filter|split-and-pipe|spp|\

