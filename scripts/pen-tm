    shortcuts) {
        paths="$(pen-tm-list-shortcuts)"

        # pen-tm dv "$paths"
        if [ -n "$paths" ]; then

            # Now this is the slowest thing
            # pen-tm -f -i nw "mfz -nm"
            # Can substitute for mfz -nm

            pl "$paths" | s uniq | pen-tm -f -i -S nw -noerror "mfz -nm" | {
                dn="$(cat|umn)"
                dnq="$(printf -- "%s" "$dn" | q)"
                # dn="$(realpath "$dn")"
                # echo "$dn" > /tmp/paths.txt
                # exit 0
                exec <`pen-tm-tty`

                 # | u dn
                # pen-tm -i nw v
                # read dn # don't use read because it might hang
                # if [ -n "$dn" ]; then
                #    pen-tm -d -te dv "$dn"
                #    exit 0
                # fi
                # exit 0
                if [ -n "$dn" ]; then
                    if ! [ -e "$dn" ]; then
                        pen-tm -f -d -te dv "$dn :: No longer exists"
                    fi
                    export CWD="$dn"
                    # pen-tm -f -te -d nw -c "$dn" "CWD=$dnq ranger"

                    pen-tm -f -te -d nw -c "$dn" "zcd $(aqf "$dnq")"
                fi
                exit 0
            }
            exit $?
        else
            ns "No dirs exist anymore." &>/dev/null
            # This opens vim directories.org
            # If the dirs expanded no longer exist.
            pen-tm -f -te -d dir
        fi
        exit 0
