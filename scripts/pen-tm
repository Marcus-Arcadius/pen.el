        fi

        envstring+="export CWD=\"$dir\"; "

        # tf_exports="$(ux tf exports sh || echo /dev/null)"
        tf_exports="$(printf -- "%s\n" "$envstring" | tf sh || echo /dev/null)"
         # > "$tf_exports"

        # This fixes eipe
        # echo hi | pen-tm eipe | cat
        printf -- "%s\n" "export TTY=\$(tty);" >> "$tf_exports"

        # trap "rm \"$tf_exports\" 2>/dev/null" 0

        fullcmd="test -f \"$tf_exports\" && . $tf_exports; ${cmd_pre} $cmd ${cmd_post}"
        printf -- "%s\n" "$fullcmd" | ds -s pen-tm
        # exit 0

        # p "$fullcmd" | tv
        # echo "$fullcmd"

        # lit "$fullcmd"
        # pak

        # lit "$envstring ${cmd_pre} $cmd$cmd_post" 1>&2

        # lit "$fullcmd" >> /tmp/cmd.txt
        # pen-tm -d dv "$fullcmd"
        # lit "test: $PATH" >> /tmp/path.txt

        # tmux neww
        # exit 0

        # Use a python one-liner
        # py2.7 "exit(1 if 5 > 10 else 0)"
        # py2.7 "exit(0 if $COLUMNS / $LINES < 2.5 else 1)"; echo $?
        if test "$SMART_SPLIT" = "y"; then
            # if ratio of W/H < 2.5 then do an sph
            # Can't use timeout here
            if test-should-hsplit; then
                SPLIT=-v
            else
                SPLIT=-h
            fi
        fi

        # These commands are excruciatingly slow in nested tmux
        # sessions. Never use them
        # if false && ! test "$FAST" = "y"; then
        #     if test "$REMAIN_ON_EXIT" = "y"; then
        #         tmux set -t "$TARGET" set-remain-on-exit on
        #     else
