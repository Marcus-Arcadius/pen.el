        fi

        # fullcmd="$(printf -- "%s" "$fullcmd" | bs '`' | bs '`' | bs '`')"
        fullcmd="$(printf -- "%s" "$fullcmd" | esc '`')"

        printf -- "%s\n" "$fullcmd" | ds -s tm_fullcmd

        # If it's a session name then make sure it has an ending :
        if printf -- "%s\n" "$TARGET" | grep -q -P '^[a-z_]+$'; then
            TARGET="$TARGET:"
        fi

        # IMPORTANT -- The actual command running.
        # respawn
        if test "$f" = "cw"; then
            :
            # tmux neww
            pen-tm breakp -s -E "$fullcmd"
            # cmd pen-tm breakp -s -E "$fullcmd" > /tmp/yoyoht.txt
            # eval "pen-tm breakp -s $(aqf "$fullcmd")"
            # full_id="$(tmux respawnp -k -t "$TARGET" -t "$TARGET" "$fullcmd")"
        elif test "$f" = "rs" || test "$f" = "rsp"; then
            full_id="$(tmux respawnp -k -t "$TARGET" -t "$TARGET" "export TMUX=; $fullcmd")"
        elif test "$SUB_SESSION" = "y"; then
            server="$(pen-tm-get-server)"

            unset TMUX
            # -t "$TARGET"
            # cmd tmux -L $server new -F "$ID_FORMAT" -P $BACKGROUNDED -s "$session_name" -n "$window_name" -c "$dir" "$fullcmd"
            # exit 0

            # -t "$TARGET"

            if test -n "$session_name"; then
                session_name_opt=" -s $session_name "
            fi
            
            full_id="$(tmux -L $server new -P -F "$ID_FORMAT" -d $session_name_opt -n "$window_name" -c "$dir" "$fullcmd")"

            if ! test -n "$BACKGROUNDED"; then
                tmux -L $server attach -t "$full_id"
            #else
            #    echo "$full_id"
            fi

        elif test -n "$SPLIT"; then
            if test -n "$split_size"; then
                split_size="-p $split_size"
            fi
            full_id="$(tmux split -F "$ID_FORMAT" -P -t "$TARGET" $SPLIT $split_size $BACKGROUNDED -c "$dir" "$fullcmd")"
        else
            full_id="$(tmux neww -P -F "$ID_FORMAT" -t "$TARGET" $PARENT_SESSION $BACKGROUNDED -n "$window_name" -c "$dir" "$fullcmd")"
            case "$HIGHLIGHT" in
                warn) {
                    tmux setw -t "$full_id" window-status-format '#[fg=colour244]#I #[fg=colour227,bg=colour161]#W'
                }
                ;;

                red) {
                    tmux setw -t "$full_id" window-status-format '#[fg=colour244]#I #[fg=colour160,bg=colour235]#W'
                }
                ;;

                blue) {
                    tmux setw -t "$full_id" window-status-format '#[fg=colour244]#I #[fg=colour026,bg=colour235]#W'
                }
                ;;

                dblue) {
                    tmux setw -t "$full_id" window-status-format '#[fg=colour244]#I #[fg=colour024,bg=colour233]#W'
                }
                ;;

                green) {
                    tmux setw -t "$full_id" window-status-format '#[fg=colour244]#I #[fg=colour040,bg=colour235]#W'
                }
                ;;

                *)
            esac

            ## A new window was created. Was it for a subsession? If so,
            ## set the subsession's parent window id to the full_id

            #if [ -n "$PARENT_SESSION_ID" ]; then
            #    tmux setenv -t "$session_name" "PARENT_SESSION_ID" "$PARENT_SESSION_ID" \; \
            #fi
        fi

        # tmux setw -t "$full_id" window-status-format '#[fg=colour244]#I #[fg=colour227,bg=colour161]#W'

        # Never use these
        # if false && ! test "$FAST" = "y"; then
        #     tmux set -t "$TARGET" set-remain-on-exit off
        # fi

        if test "$CHANGE_SHELL" = "y"; then
            pen-tm op default-shell "$PREVIOUS_SHELL"
        fi

        # This is done later
        if test "$RETURN_ID" = "y"; then
            echo "$full_id"
        fi

        if test "$HAS_STDIN" = "y"; then
            cat > "$ff0"
            rm "$ff0" 2>/dev/null
        fi

        if test "$HAS_STDOUT" = "y"; then
            cat "$ff1" &
        fi

        if test "$DETACHMENT_SENTINEL" = "y"; then
            # Don't use the sentinel if it's a subsession and it's not
            # backgrounded. As  far as I know there is not a parameter
            # yet for this
            if ! test "$SUB_SESSION" = "y"; then
                tmux wait-for "$sentinal_string"
            fi
        fi

        if test "$CAT_ON_EXIT" = "y"; then
            cat "$tf_capture"
        fi

        # This sleep is needed because the last it of stdout is yet to
        # come through
        # echo -n "hi\nyo" | pen-tm filter | cat
        sleep 0.1

        if [ -z "$RETURN_CODE" ]; then
            RETURN_CODE="$(cat "$tf_rc")"
            if [ -z "$RETURN_CODE" ]; then
                RETURN_CODE=99
            fi
        fi
        # echo "$RETURN_CODE"

        trap "rm \"$tf_rc\" 2>/dev/null" 0

        # Need to implement return code

        if [ -e "$ff1" ]; then
            rm "$ff1" 2>/dev/null
        fi
        # exit 0
        exit "$RETURN_CODE"
    }
