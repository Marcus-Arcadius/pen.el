second_last_arg() {
    if [ "$#" -gt "1" ]; then
        n=$(($#-1))
        eval ARG=\${$n}
        pl "$ARG"
    fi
}

last_arg() {
    if [ "$#" -gt "0" ]; then
        n=$(($#-0))
        eval ARG=\${$n}
        pl "$ARG"
    fi
}

session_list() {
    tmux list-sessions | sed '/^[^:]\+:/ s/^\([^:]\+\).*/\1/'
    return 0
}

copy-line() {
    y="$1"

    # offset must be a negative number, actually

    if printf -- "%s\n" "$y" | grep -q -P '^[-+]'; then
        offset="$y"
        y=
    fi
    : ${y:="$(tmux display -p "#{cursor_y}")"}

    if test -n "$offset"; then
        y="$(( y + offset ))"
    fi

    : ${y:="$(tmux display -p "#{cursor_y}")"}

    pen-tm -f cat | sed -n "$((y + 1))p" | {
        if test "$DO_FROM" = "y"; then
            : ${x:="$(tmux display -p "#{cursor_x}")"}
            cut -c $(( x + 1 ))-
        else
            # make a list of possible prompts
            clean-repl
        fi
    } | xc -i -n
}

