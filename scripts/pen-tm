    get-last-output) {
        pattern="$1"
        : ${pattern:="»"}

        pen-tm -f capture-stdout | remove-trailing-blank-lines | sed '/^$/d' | tac | sed -n -e 1d -e "0,/$pattern/p" | tac | sed 1d
    }
