            pl "$cmd"
            eval "$cmd"
            echo

            if ! test "$FAST" = "y"; then
                pl "Open files"
                pstree -lAp "$pid" | \
                    sed 's/\(([0-9]\+)\)/(\n\1\n/g' | \
                    sed -n '/([0-9]\+)/ s/[^0-9]//pg' | \
                    while read pid; do
                        lsof -p $pid 2>/dev/null | sed 's/ \+/ /g' | cut -d ' ' -f9- | sed '1d';
                    done | s uniq | grep -v /lib/
            else
                echo Fast mode enabled. Not showing open files.
            fi

            # lsof

        } | mnm | {
