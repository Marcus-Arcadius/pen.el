        PARENT_PANE_ID="$(tmux show-env -t "$target" PARENT_WINDOW_ID | sed 's/.*=//')"
        # echo "parent: $PARENT_PANE_ID" >> /tmp/pen-tm.log
        if [ -n "$PARENT_PANE_ID" ]; then
            pen-tm sel "$PARENT_PANE_ID"
        fi

        exit $?

        # if [[ "${target}" =~ .*:.*\..* ]]; then
            # lit "session:window.pane"
        # elif [[ "${target}" =~ [^:]*\..* ]]; then
            # lit "window.pane"
        # elif [[ "${target}" =~ .*:[^.]* ]]; then
