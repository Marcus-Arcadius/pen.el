                case "$ch" in
                    $'\04') got_eot=1   ;&
                    $'\n')  break       ;;
                    *)      line="$line$ch" ;;
                esac
            done

            printf 'line: "%s"\n' "$line"

            if (( got_eot )); then
                break
            fi
        done

        # keys="$(cat)"

        target=
        while [ $# -gt 0 ]; do opt="$1"; case "$opt" in
            -t) {
                target="$2"
                shift
                shift
