    edit-file) {
        paths="$(find . -maxdepth 1 -type f | sed 's/^..//')"
        if [ -n "$paths" ]; then
            paths="$(pl "$paths" | s uniq | pen-tm -f -i spv -noerror "mfz -p" | umn)"
            pl "$paths" | awk1 | while IFS=$'\n' read -r line; do
                if test -n "$line"; then
                    pen-tm tp "$line"
                fi
            done

            #paths="$(pl "$paths" | s uniq | pen-tm -f -i spv -noerror "mfz -p" | umn | q -ftln | tr -s '\n' ' ')"
            #if [ -n "$paths" ]; then
