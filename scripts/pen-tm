    open-dirs) {
        selections="$(pen-tm -s -fout spv -noerror "F find-no-git -d | mfz")"
        if [ -n "$selections" ]; then
            printf -- "%s\n" "$selections" | \
            while read -r line; do
                if [ -e "$line" ]; then
                    line="$(p "$line" | qne)"
                    bn="$(basename "$line")"
                    pen-tm -te -d nw -noerror -n "$bn" "open -e $line"
                fi
            done
        fi
    }
    ;;

    open-files) {
        # selections="$(pen-tm -s -fout spv -noerror "F find-no-git -f | mfz")"
        selections="$(pen-tm -s -fout spv -noerror "F f | mfz")"
        if [ -n "$selections" ]; then
            printf -- "%s\n" "$selections" | \
            while read -r line; do
                if [ -e "$line" ]; then
                    line="$(p "$line" | qne)"
                    bn="$(basename "$line")"
                    pen-tm -te -d nw -noerror -n "$bn" "open -e $line"
                fi
            done
        fi
    }
    ;;

    mfz|fzf) {
        pen-tm -S -sout sps -args -noerror "$f" "$@" | cat
        exit $?
    }
    ;;

    less) {
        pen-tm -S -tout spv -noerror tless 
        exit $?
    }
    ;;

    filter) {
        IFS= read -rd '' input < <(cat /dev/stdin)

        # Due to some mechanism, I don't understand, C-c on this will
        # not give an error exit code
        # Maybe the f is still running
        # sout="$(printf -- "%s" "$input" | pen-tm spv "f filter-with-fzf | cat" | cat)"

        export HAS_STDOUT
        sout="$(set -o pipefail; printf -- "%s" "$input" | pen-tm spv -noerror "f filter-with-fzf" | cat)"
        ret="$?"
        # echo "$sout"
        # echo "$ret"

        if test "$ret" -eq 0; then
            {
                if [ -n "$sout" ]; then
                    printf -- "%s" "$sout"
                else
                    printf -- "%s" "$input"
                fi
            } | {
                if test "$HAS_STDOUT" = "y"; then
                    cat
                else
                    # Use tv instead of vim. Helps (defun my/fwfzf)
                    tv
                fi
            }
        fi

        exit "$ret"
    }
