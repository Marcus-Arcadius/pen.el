#!/bin/bash
export TTY

# nsfa needs to be created from within elisp

: ${modename:="$1"}
modename=" \"$modename\""
# script="$(pen-nsfa "$@")"
bufname="$(p term "$@" | slugify)"

# elisp="(pen-term \"$script\" t $modename)$elisp"
elisp="(pen-term (pen-nsfa $(cmd-nice "$(cmd-nice-posix "$@")")) t $modename)$elisp"

# oresp -sh "$@"
pent "$elisp"
